        </primitive>

        <primitive id="bWallPrim">
            <rectangle x1="0" y1="0" x2="20" y2="2.5"/>
        </primitive>

        <primitive id="cornerWallPrim">
            <rectangle x1="0" y1="0" x2="6.124" y2="2.5"/>
        </primitive>

        <primitive id="cStandPrim">
            <rectangle x1="0" y1="0" x2="32" y2="5"/>
        </primitive>

        <primitive id="bStandPrim">
            <rectangle x1="0" y1="0" x2="20" y2="5"/>
        </primitive>

        <primitive id="cornerStandPrim">
            <triangle x1="4.33" y1="2.5" z1="0" x2="0" y2="2.5" z2="-4.33" x3="0" y3="0" z3="0"/>
        </primitive>

        <primitive id="goalPostPrim">
            <cylinder base="0.5" top="0.5" height="1" slices="50" stacks="20"/>
        </primitive>

        <primitive id="lightPostPrim">
            <cylinder base="0.8" top="0.2" height="4" slices="50" stacks="20"/>
        </primitive>

        <primitive id="ballPrim">
            <sphere radius="1" slices="50" stacks="50"/>
        </primitive>

        <primitive id="patchPrim">
            <patch orderU="2" orderV="3" partsU="20" partsV="20">
                <controlpoint x="-1.5" y="-1.5" z="0.0"/>
                <controlpoint x="-2.0" y="-2.0" z="2.0"/>
                <controlpoint x="-2.0" y="2.0" z="2.0"/>
                <controlpoint x="-1.5" y="1.5" z="0.0"/>
                <controlpoint x="0.0" y="0.0" z="3.0"/>
                <controlpoint x="0.0" y="-2.0" z="3.0"/>
                <controlpoint x="0.0" y="2.0" z="3.0"/>
                <controlpoint x="0.0" y="0.0" z="3.0"/>
                <controlpoint x="1.5" y="-1.5" z="0.0"/>
                <controlpoint x="2.0" y="-2.0" z="2.0"/>
                <controlpoint x="2.0" y="2.0" z="2.0"/>
                <controlpoint x="1.5" y="1.5" z="0.0"/>
            </patch>
        </primitive>

        <primitive id="planePrim">
            <plane dimX="2" dimY="1" partsX="20" partsY="20"/>
        </primitive>

        <primitive id="vehiclePrim">
            <vehicle/>
        </primitive>

    </primitives>

    <components>

        <component id="root">

            <!-- bloco "transformation" obrigatorio -->
            <transformation>
                <scale x="1" y="1" z="1"/>
            </transformation>

            <animation>
                <!--animationref id="oiCircular"/-->
                <animationref id="oiCircular"/>
            </animation>

            <materials>
                <material id="m1"/>

            </materials>

            <texture id="none"/>

            <children>
                <componentref id="stands"/>
                <componentref id="walls"/>
                <componentref id="goals"/>
                <componentref id="lights"/>
                <componentref id="plane"/>
                <componentref id="patch"/>
                <componentref id="vehicle"/>
            </children>

        </component>

        <component id="plane">

            <!-- bloco "transformation" obrigatorio -->
            <transformation>
                <translate x="0" y="0.1" z="0"/>
            </transformation>

            <materials>

                <material id="ballMat"/>

            </materials>

            <texture id="ballTex"/>

            <children>
                <primitiveref id="planePrim"/>
            </children>

        </component>

        <component id="patch">

            <!-- bloco "transformation" obrigatorio -->
            <transformation>
                <translate x="0" y="3" z="0"/>
            </transformation>

            <materials>

                <material id="ballMat"/>

            </materials>

            <texture id="ballTex"/>

            <children>
